select distinct(ticket.tn),substring(ticket.title,1,70), ticket.change_time,ticket_history.state_id,
ticket_history.history_type_id,ticket_history.owner_id, users.last_name as owner
from ticket, ticket_history, users
where ticket.id=ticket_history.ticket_id 
and (ticket_history.history_type_id=8 or ticket_history.history_type_id=15)
and ticket_history.owner_id=users.id
and ticket_history.state_id not in (2, 3, 5, 9, 17)
group by ticket.tn order by ticket.change_time asc;
